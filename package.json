{
  "name": "addok-geocode-stream",
  "version": "0.11.0",
  "description": "Node.js stream API and CLI for addok geocoder",
  "main": "index.js",
  "repository": "https://github.com/jdesboeufs/addok-geocode-stream",
  "author": "Jérôme Desboeufs <jerome.desboeufs@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "bin": {
    "addok-geocode": "cli.js"
  },
  "scripts": {
    "lint": "xo",
    "test": "ava"
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "csv-write-stream": "^2.0.0",
    "form-data": "^4.0.0",
    "get-stream": "^6.0.1",
    "got": "^11.8.3",
    "iconv-lite": "^0.6.3",
    "jschardet": "^3.0.0",
    "lodash-es": "^4.17.21",
    "mississippi": "^4.0.0",
    "strip-bom-stream": "^5.0.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "ava": "^4.3.3",
    "xo": "^0.52.3"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/no-array-for-each": "off",
      "unicorn/text-encoding-identifier-case": "off"
    }
  },
  "engines": {
    "node": ">= 16"
  }
}
